		aw8672x_cont_drv1_time = /bits/ 8 < 0x04 >;
		aw8672x_cont_drv2_time = /bits/ 8 < 0x14 >;
		aw8672x_duration_time = /bits/ 8 < 20 30 60 >;
		aw8672x_d2s_gain = /bits/ 8 < 0x04 >;
		aw8672x_trig_config = /bits/ 8 < 0 0 0 1 0 2 0 0
						 0 0 0 1 0 2 0 0
						 0 0 0 1 0 2 0 0 >;
		aw8672x_bst_vol_default = < 8000 >;
		aw8672x_is_enabled_track_en;
		/* aw8672x_is_enabled_auto_bst; */
		/* aw8672x_is_enabled_one_wire; */

/***************************** aw8692x ******************************/
		aw8692x_f0_cali_percent = /bits/ 8 < 7 >;
		aw8692x_gain_bypass = /bits/ 8 < 1 >;
		aw8692x_cont_drv1_lvl = /bits/ 8 < 0x7F >;
		aw8692x_cont_lra_vrms = < 1000 >; /* Motor rated voltage, mV. If use 0812, 0512, 0412 or 0825, please select ram get f0 */
		aw8692x_cont_drv1_time = /bits/ 8 < 0x04 >;
		aw8692x_cont_drv2_time = /bits/ 8 < 0x14 >;
		aw8692x_cont_track_margin = /bits/ 8 < 0x0F >;
		aw8692x_cont_brk_gain = /bits/ 8 < 0x08 >;
		aw8692x_cont_brk_time = /bits/ 8 < 0x08 >;
		aw8692x_duration_time = /bits/ 8 < 20 30 60 >;
		aw8692x_d2s_gain = /bits/ 8 < 0x04 >;
