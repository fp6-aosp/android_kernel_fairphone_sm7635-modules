&tlmm {
	gpio49_input_pull_up: gpio49_input_pull_up {
		mux {
			 pins = "gpio49";
			 function = "gpio";
		};

		config {
			pins = "gpio49";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};
};
