		aw8693xs_f0_cali_percent = /bits/ 8 < 10 >;
		aw8693xs_gain_bypass = /bits/ 8 < 1 >;
		aw8693xs_uvlo_vol_default = /bits/ 8 < 4 >;
		aw8693xs_cont_drv1_lvl = /bits/ 8 < 0x7F >;
		aw8693xs_cont_lra_vrms = < 1000 >;
		aw8693xs_cont_drv1_time = /bits/ 8 < 0x04 >;
		aw8693xs_cont_drv2_time = /bits/ 8 < 0x14 >;
		aw8693xs_cont_track_margin = /bits/ 8 < 0x0F >;
		aw8693xs_cont_brk_gain = /bits/ 8 < 0x08 >;
